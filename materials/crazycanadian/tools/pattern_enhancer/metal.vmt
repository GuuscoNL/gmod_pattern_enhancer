"VertexlitGeneric"
{
	$basetexture  crazycanadian\tools\pattern_enhancer\enhancer_diffuse
	$bumpmap crazycanadian\tools\pattern_enhancer\enhancer_normal
	
	$envmap env_cubemap
	$envmapcontrast 1
	$envmapsaturation 1
	$envmaptint "[ 0.05 0.05 0.05]"

	$phong 1
   	$phongexponent 2	
    $phongboost 10
    $phongfresnelranges "[0 0.5 1]"
}
